{
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.meta": true
  },
  "search.exclude": {
    "**/__pycache__": true,
    "**/*.meta": true
  },
  "files.associations": {
    ".github/CODEOWNERS": "plaintext"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": "explicit"
    },
    "editor.inlayHints.enabled": "off"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[postgres]": { "editor.defaultFormatter": "bradymholt.pgformatter" },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "SashaBusinaro.yaml-compose-sorter",
    "editor.formatOnSave": true
  },
  "yaml-compose-sorter.topLevelKeyOrder": [
    "version",
    "name",
    "x-base-images",
    "x-base-args",
    "x-registry-cache",
    "networks",
    "volumes",
    "services",
    "secrets",
    "configs"
  ],
  "yaml-compose-sorter.serviceKeyOrder": [
    "profiles",
    "container_name",
    "x-image-ref",
    "image",
    "build",
    "depends_on",
    "user",
    "restart",
    "ports",
    "expose",
    "volumes",
    "networks",
    "devices",
    "group_add",
    "gpus",
    "extra_hosts",
    "logging",
    "command",
    "entrypoint",
    "healthcheck",
    "env_file",
    "environment"
  ],
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.theme": "classic",
  "editor.formatOnSave": true,
  "dotnet.enableWorkspaceBasedDevelopment": true,
  "debug.onTaskErrors": "debugAnyway",
  "python.languageServer": "None",
  "python.experiments.enabled": false,
  "python.experiments.optOutFrom": ["All"],
  "basedpyright.analysis.diagnosticMode": "workspace",
  "pgFormatter.configFile": "${workspaceFolder}/pg_format.conf",
  "ruff.nativeServer": "on",
  "ruff.importStrategy": "useBundled",
  "Devdb.enableMcpServer": false,
  "Devdb.colorTheme": "dark"
}
