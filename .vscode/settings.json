{
  "files.exclude": {
    "**/__pycache__": true,
  },
  "search.exclude": {
    "**/__pycache__": true,
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": "explicit"
    }
  },
  "python.languageServer": "Pylance",
  "python.analysis.languageServerMode": "default",
  "python.analysis.diagnosticMode": "workspace"
}
